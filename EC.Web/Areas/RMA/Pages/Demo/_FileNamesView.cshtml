@{
    List<string> fileNames = new List<string>();
    if (ViewData["FileNames"] != null)
    {
        fileNames = (List<string>)ViewData["FileNames"];
    }
}
<ul>
    @if (fileNames != null)
    {
        @foreach (var fileName in fileNames)
        {
            <li>@fileName</li>
        }
    }
</ul>